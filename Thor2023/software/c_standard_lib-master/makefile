	nmake /C LOCALE
	cd ..
#
MATH:
	cd MATH
	nmake /C MATCH
	cd ..
#
SETJMP:
	cd SETJMP
	nmake /c SETJMP
	cd ..
#
SIGNAL:
	cd SIGNAL
	nmake /C SIGNAL
	cd ..
#
STDIO:
	cd STDIO
	nmake /C STDIO
	cd ..
#
STDLIB:
	cd STDLIB
	nmake /C STDLIB
	cd ..
#
STRING:
	cd STRING
	nmake /C STRING
	cd ..
#
TIME:
	cd TIME
	nmake /C TIME
	cd ..

clean:
	cd ASSERT
	nmake /C clean
	cd ..
	cd CTYPE
	nmake /C clean
	cd ..
	cd ERRNO
	nmake /C clean
	cd ..
	cd FLOAT
	nmake /C clean
	cd ..
	cd LOCALE
	nmake /C clean
	cd ..
	cd MATH
	nmake /C clean
	cd ..
	cd SETJMP
	nmake /C clean
	cd ..
	cd SIGNAL
	nmake /C clean
	cd ..
	cd STDIO
	nmake /C clean
