# Makefile for Standard C Library
#
CXX = cc64 -w -S -fno-exceptions

all:
	cd ASSERT
	nmake /C ASSERT
	cd ..
	cd CTYPE
	nmake /C CTYPE
	cd ..
	cd ERRNO
	nmake /C ERRNO
	cd ..
	cd FLOAT
	nmake /C FLOAT
	cd ..
	cd LOCALE
	nmake /C LOCALE
	cd ..
	cd MATH
	nmake /C MATH
	cd ..
	cd SETJMP
	nmake /c SETJMP
	cd ..
	cd SIGNAL
	nmake /C SIGNAL
	cd ..
	cd STDIO
	nmake /C STDIO
	cd ..
	cd STDLIB
	nmake /C STDLIB
	cd ..
	cd STRING
	nmake /C STRING
	cd ..
	cd TIME
	nmake /C TIME
	cd ..


ASSERT:
	cd ASSERT
	nmake /C ASSERT
	cd ..
